      "build": "wasaby-cli --tasks=build --only",
      "start": "wasaby-cli --tasks=app"
   },
   "wasaby-cli": {
      "port": 8080
   },
   "devDependencies": {
      "cookie-parser": "^1.4.3",
      "express": "^4.16.4",
      "wasaby-cli": "git+https://github.com/saby/wasaby-cli.git#rc-20.4100",
      "@types/cookie-parser": "^1.4.2",
      "@types/express": "^4.17.1",
      "@types/node": "^12.11.1"
   }
}
