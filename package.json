{
   "name": "wasaby-phonebook",
   "version": "20.4100.0",
   "license": "MS-RL",
   "repository": {
      "type": "git",
      "url": "https://github.com/saby/wasaby-phonebook.git"
   },
   "scripts": {
      "update-cli-store": "wasaby-cli --tasks=initStore --only",
      "build:typescript": "saby-typescript --install --mode=development",
